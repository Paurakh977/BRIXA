{
  "name": "Brixa",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start":"pnpm build && turbo run start",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "turbo run db:generate",
    "db:migrate": "pnpm --filter database db:migrate",
    "db:push": "turbo run db:push",
    "db:pull": "turbo run db:pull",
    "db:deploy": "turbo run db:deploy",
    "db:reset": "pnpm --filter database db:reset",
    "db:seed": "turbo run db:seed"
  },
  "devDependencies": {
    "@BRIXA/eslint-config": "workspace:*",
    "@BRIXA/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "turbo": "^2.6.1"
  },
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18"
  }
}
